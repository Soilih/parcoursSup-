

{% extends 'base.html.twig' %}
{% block body %}
<div class="wrapper">
  <div class="content-wrapper">
     <div class="row" style="padding-left:2%;padding-top:2%">
          <div class="col-md-12">
            <div class="card card-success">
              <div class="card-header">
                <h3 class="card-title">
                  LISTE DES RESPONSABLES 
                   </h3>
                    {# <a  href="{{ path("ecole_new") }}" class="btn btn-primary" style="float:right">Ajouter une ecole </a> #}
                </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table  id="datatable" class="table table-bordered table-hover">
               
                    <thead>
                        <tr>
                            <th>Nom de famille </th>
                            <th>Prenom  </th>
                            <th>Adresse domicile </th>
                            <th>Telephone</th>
                            <th>Adresse Email</th>
                            <th>Nom & prenom etudiant   </th>
                            <th> Revenu mensuel responsable  </th>
                            <th> Proffession   </th>
                           
                        </tr>
                    </thead>
                    <tbody>
                    {% for et in nos_etudiant %}
                        <tr>
                        
                            <td>{{ et.n}}</td>
                             <td>{{ et.pr }}</td>
                             <td>{{ et.ad}}</td>
                             <td>{{ et.t}}</td>
                             <td>{{ et.em }}</td>
                             <td>{{ et.nm }} {{ et.prnm}}</td>
                             <td>{{ et.rev }} KMF </td> 
                              <td>{{ et.pro}}</td> 
                             
                            
                            
                        </tr>
                    {% else %}
                        <tr>
                            <td colspan="8">no records found</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                

                </table>
              </div>
              <!-- /.card-body -->
            </div>
          </div>  
        </div>
    </div>
</div>
{% endblock %}